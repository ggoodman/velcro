{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "composite": true,
    "declaration": true,
    "declarationMap": true,
    "downlevelIteration": true,
    "esModuleInterop": true,
    "incremental": true,
    "lib": ["es2018"],
    "module": "commonjs",
    "moduleResolution": "node",
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "pretty": true,
    "resolveJsonModule": true,
    "rootDir": "./",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "stripInternal": true,
    "target": "ESNext"
  },
  "files": [],
  "references": [
    { "path": "./packages/@velcro/bundler" },
    { "path": "./packages/@velcro/common" },
    { "path": "./packages/@velcro/node-libs" },
    { "path": "./packages/@velcro/resolver" },
    { "path": "./packages/@velcro/runner" },
    { "path": "./packages/@velcro/strategy-cdn" },
    { "path": "./packages/@velcro/strategy-compound" },
    { "path": "./packages/@velcro/strategy-fs" },
    { "path": "./packages/@velcro/strategy-memory" }
  ]
}
