{
  "name": "nostalgie",
  "version": "0.37.0",
  "description": "A reminder of what the web once was - your hopes and dreams and a single html file",
  "keywords": [
    "velcro",
    "resolve",
    "npm",
    "resolution",
    "module",
    "browser",
    "css"
  ],
  "author": "Geoff Goodman <ggoodman@gmail.com>",
  "homepage": "https://github.com/ggoodman/velcro",
  "license": "MIT",
  "browser": "./dist/index.browser.js",
  "main": "./dist/index.js",
  "module": "./dist/index.mjs",
  "unpkg": "./dist/index.umd.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ggoodman/velcro.git"
  },
  "scripts": {
    "build": "rollup -c",
    "build:api": "api-extractor run --local -c ./api-extractor.json",
    "build:readme": "cat README.md | sed \"s~tree/.*/docs~tree/v$(jq -r .version package.json)/docs~g\" | sed \"s~nostalgie@.*/~nostalgie@$(jq -r .version package.json)/~g\" > README.md",
    "preversion": "npm run build",
    "version": "npm run build:api && npm run build:readme"
  },
  "prettier": {
    "printWidth": 100,
    "singleQuote": true
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.8.1",
    "@microsoft/api-extractor": "^7.8.1",
    "@rollup/plugin-alias": "^3.1.0",
    "@rollup/plugin-commonjs": "^11.1.0",
    "@rollup/plugin-json": "^4.0.3",
    "@rollup/plugin-replace": "^2.3.2",
    "@velcro/bundler": "^0.37.0",
    "@velcro/common": "^0.37.0",
    "@wessberg/rollup-plugin-ts": "^1.2.24",
    "magic-string": "^0.25.7",
    "rollup": "^2.10.0",
    "rollup-plugin-inject-process-env": "^1.2.0",
    "rollup-plugin-terser": "^5.3.0",
    "tslib": "1",
    "typescript": "^3.9.2"
  },
  "dependencies": {
    "@velcro/bundler": "^0.37.0",
    "@velcro/node-libs": "^0.37.0",
    "@velcro/plugin-css": "^0.37.0",
    "@velcro/plugin-sucrase": "^0.37.0",
    "@velcro/resolver": "^0.37.0",
    "@velcro/strategy-cdn": "^0.37.0",
    "@velcro/strategy-compound": "^0.37.0",
    "@velcro/strategy-memory": "^0.37.0",
    "sucrase": "^3.15.0"
  },
  "gitHead": "f63b45ad688f2fe9768349afeaf449c016bf33ef"
}
