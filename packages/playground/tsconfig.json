{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "scripthost",
      "webworker"
    ],
    "allowJs": true,
    "downlevelIteration": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "react",
    "noEmit": true
  },
  "exclude": [
    "**/node_modules/**/*"
  ],
  "include": [
    "src"
  ],
  "references": [
    {
      "path": "../@velcro/bundler"
    },
    {
      "path": "../@velcro/common"
    },
    {
      "path": "../@velcro/plugin-css"
    },
    {
      "path": "../@velcro/plugin-sucrase"
    },
    {
      "path": "../@velcro/resolver"
    },
    {
      "path": "../@velcro/strategy-cdn"
    },
    {
      "path": "../@velcro/strategy-compound"
    },
    {
      "path": "../@velcro/strategy-fs"
    },
    {
      "path": "../@velcro/strategy-memory"
    }
  ]
}
